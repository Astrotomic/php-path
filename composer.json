{
    "name": "astrotomic/path",
    "type": "library",
    "description": "An enhanced node.js path module port for PHP.",
    "keywords": [
        "path",
        "nodejs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Pock",
            "email": "self@danpock.me",
            "homepage": "https://danpock.me",
            "role": "Developer"
        },
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://astrotomic.info",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "pestphp/pest": "dev-master",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\Path\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\Path\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
